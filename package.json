{
  "name": "prompttrail-ts",
  "private": true,
  "version": "0.0.1",
  "description": "PromptTrail: A lightweight library for Prompt Programming, Agent as Code",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "vitest --run --watch=false",
    "readme:test": "node scripts/generate-readme-tests.js && vitest run --config vitest.readme.config.ts readme.test.ts",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint:check && pnpm run format:check",
    "context": "bun run dump_context.ts packages/core/ -- md ts",
    "pad": "bun run scratchpad.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^20.17.32",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "clipboardy": "^4.0.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "ignore": "^7.0.4",
    "madge": "^8.0.0",
    "prettier": "^3.5.3",
    "tiktoken": "^1.0.21",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^1.6.1",
    "zod": "^3.24.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/openai": "^1.3.21",
    "@anthropic-ai/sdk": "^0.36.3",
    "@prompttrail/core": "link:packages/core",
    "ai": "^4.3.13",
    "nanoid": "^5.1.5",
    "openai": "^4.97.0",
    "zustand": "^5.0.4"
  },
  "pnpm": {
    "overrides": {
      "@prompttrail/core": "link:packages/core"
    }
  }
}
