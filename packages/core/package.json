{
  "name": "@prompttrail/core",
  "version": "0.0.1",
  "description": "Core functionality for PromptTrail TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --watch=false",
    "test:cline": "tmpfile=\"/tmp/vitest_output_$(date +%s%N).log\" && pnpm exec vitest --run --watch=false > \"$tmpfile\" 2>&1 && cat \"$tmpfile\" && rm \"$tmpfile\"",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.1",
    "prettier": "^3.5.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.18",
    "@ai-sdk/openai": "^1.2.6",
    "@anthropic-ai/sdk": "^0.36.3",
    "@modelcontextprotocol/sdk": "^1.6.0",
    "ai": "^4.1.63",
    "openai": "^4.85.4",
    "zod": "^3.24.2"
  }
}
